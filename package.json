{
    "name": "consensource-ui",
    "version": "0.1.0",
    "description": "ConsenSource UI",
    "scripts": {
        "start": "npm run generate-protobufs && react-scripts start && mv src/react-app-env.d.ts src/types",
        "build": "npm run generate-protobufs && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "clean": "rm -f public/js/*.js",
        "generate-protobufs": "./scripts/compile_protobufs.sh",
        "lint": "npm run generate-protobufs && eslint 'src/**/*.{js,ts,tsx}' --fix --ignore-pattern 'src/protobuf/*'",
        "start:dev": "webpack-dev-server --open --hot"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/target/consensource-ui"
    },
    "keywords": [
        "webclient"
    ],
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@types/eventsource": "^1.1.2",
        "@types/jest": "^25.1.4",
        "@types/node": "^13.13.0",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-router-dom": "^5.1.4",
        "@types/sawtooth-sdk": "^1.1.0",
        "@types/sjcl": "^1.0.29",
        "@types/uuid": "^7.0.0",
        "@types/webpack": "^4.41.8",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "jest-localstorage-mock": "^2.4.0",
        "prettier": "^1.19.1",
        "protobufjs": "^6.8.8",
        "react-scripts": "^3.4.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.11",
        "eventsource": "^1.0.7",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2",
        "sawtooth-sdk": "^1.0.5",
        "sjcl": "^1.0.7",
        "uuid": "^3.3.2"
    },
    "loglevel": "error",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
